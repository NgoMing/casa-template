<p id="notice"><%= notice %></p>

<h1>Clause Masters</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clause_masters.each do |clause_master| %>
      <tr>
        <td><%= link_to 'Show', clause_master %></td>
        <td><%= link_to 'Edit', edit_clause_master_path(clause_master) %></td>
        <td><%= link_to 'Destroy', clause_master, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Clause Master', new_clause_master_path %>
